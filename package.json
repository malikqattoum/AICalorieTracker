  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run build:server && node dist/index.js",
    "dev:client": "cd client && vite",
    "build": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "test": "jest --config jest.config.cjs",
    "test:migrations": "jest --config jest.config.cjs server/src/tests/migration.test.ts",
    "test:ci": "jest --ci --coverage",
    "build:production": "expo build:production",
    "deploy:production": "expo publish --release-channel production",
    "monitor:production": "sentry-cli releases deploys $RELEASE_NAME list"
  },
